	<parameter name="BUSCA_POR_ESTADO" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT lavajato.cidade.codigo,
	lavajato.cidade.nome,
	lavajato.estado.sigla
FROM lavajato.cidade
	INNER JOIN lavajato.estado ON 
	 lavajato.cidade.estado_codigo = lavajato.estado.codigo 
WHERE 
	 lavajato.estado.nome LIKE $P{BUSCA_POR_ESTADO} 
ORDER BY lavajato.cidade.codigo ASC]]>
	</queryString>
